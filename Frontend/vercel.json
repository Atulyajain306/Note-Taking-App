{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://note-taking-app-backend-sooty.vercel.app/api/$1"
    }
  ]
}

  